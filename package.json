{
  "name": "phenomic",
  "version": "0.12.1",
  "description": "Modern static website generator to create dynamic websites using React components.",
  "keywords": [
    "react",
    "ssg",
    "static",
    "file",
    "site",
    "website",
    "blog",
    "generator",
    "markdown",
    "jekyll",
    "wintersmith",
    "blacksmith",
    "metalsmith",
    "gatsby"
  ],
  "author": "Maxime Thirouin",
  "license": "MIT",
  "repository": "https://github.com/MoOx/phenomic.git",
  "main": "lib/index.js",
  "files": [
    "npm",
    "lib",
    "src",
    "boilerplate",
    "docs/content",
    "!**/__tests__"
  ],
  "bin": {
    "phenomic": "npm/bin.js"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-register": "^6.6.5",
    "chalk": "^1.1.0",
    "classnames": "^2.2.3",
    "connect-history-api-fallback": "^1.2.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.26.4",
    "fs-promise": "^0.3.1",
    "globby": "^4.0.0",
    "gray-matter": "^2.0.0",
    "inquirer": "^1.0.0",
    "loader-utils": "^0.2.11",
    "lodash.template": "^4.2.2",
    "lru-memoize": "^1.0.0",
    "mkdirp": "^0.5.1",
    "opn": "^4.0.2",
    "pify": "^2.3.0",
    "portfinder": "^1.0.2",
    "redbox-react": "^1.2.2",
    "remark": "^4.1.1",
    "remark-autolink-headings": "^3.0.0",
    "remark-highlight.js": "^3.0.0",
    "remark-html": "^3.0.0",
    "remark-slug": "^4.1.0",
    "rimraf": "^2.4.2",
    "rss": "^1.2.0",
    "simple-json-fetch": "^1.0.1",
    "source-map-support": "^0.4.0",
    "strip-markdown": "^0.3.1",
    "underscore.string": "^3.2.3",
    "valid-url": "^1.0.9",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-error-notification": "^0.1.4",
    "webpack-hot-middleware": "^2.0.0",
    "yargs": "^4.3.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0-beta.0",
    "babel-plugin-flow-react-proptypes": "^0.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "cmd-shim": "^2.0.2",
    "coveralls": "^2.11.8",
    "eslint": "^2.0.0",
    "eslint-config-i-am-meticulous": "^4.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-flow-vars": "^0.3.0",
    "eslint-plugin-react": "^5.1.1",
    "exec-cmd": "^2.0.1",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.1",
    "flow-bin": "^0.25.0",
    "flow-interfaces": "^0.5.0",
    "js-beautify": "^1.5.10",
    "jsdom": "^8.1.0",
    "lnfs": "^3.0.0",
    "mock-fs": "^3.8.0",
    "npm-run-all": "^2.1.1",
    "npmpub": "^3.0.0",
    "nyc": "^6.4.3",
    "react": "^15.0.0-rc.1",
    "react-addons-test-utils": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-helmet": "^3.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.2",
    "redux": "^3.0.0",
    "sinon": "^1.17.3",
    "stylelint": "^6.0.0",
    "stylelint-config-standard": "^7.0.0",
    "suppose": "^0.5.2"
  },
  "peerDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "invariant": "^2.1.1",
    "history": "^2.0.0",
    "react": "^0.14.0 || ^15.0.0-rc.1",
    "react-dom": "^0.14.0 || ^15.0.0-rc.1",
    "react-helmet": "^3.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.2",
    "redux": "^3.0.0",
    "webpack": "^1.12.1",
    "whatwg-fetch": "^0.11.0"
  },
  "optionalPeerDependencies": {
    "babel-eslint": "^6.0.0-beta.0",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.0.0",
    "eslint-config-i-am-meticulous": "^4.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "json-loader": "^0.5.2",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.7.0",
    "postcss-reporter": "^1.3.0",
    "npm-run-all": "^1.7.0",
    "raw-loader": "^0.5.1",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "style-loader": "^0.12.3",
    "stylelint": "^6.0.0",
    "stylelint-config-standard": "^7.0.0"
  },
  "scripts": {
    "postinstall": "node npm/postinstall.js",
    "transpile": "babel --ignore __tests__ --copy-files src --out-dir lib",
    "prepublish": "rimraf lib && npm run transpile",
    "#lint:js:eslint": "https://github.com/eslint/eslint/issues/5679",
    "lint:js:eslint": "eslint --ignore-path .gitignore --fix src __tests__ scripts npm interfaces docs/scripts docs/src docs/web_modules boilerplate/scripts boilerplate/src boilerplate/web_modules",
    "lint:js:flow": "babel-node scripts/flow-check.js",
    "lint:js": "npm-run-all --parallel lint:js:*",
    "lint:css": "stylelint \"+(boilerplate|docs)/web_modules/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "tests": "ava",
    "integration-tests": "ava __tests__/*.js",
    "pretest-boilerplate": "npm run transpile",
    "test-boilerplate": "babel-node scripts/test-boilerplate.js",
    "posttest-boilerplate": "npm run integration-tests",
    "docs": "babel-node scripts/docs.js",
    "predocs-start": "babel-node scripts/docs.js",
    "docs-start": "cd docs && npm start",
    "predocs-deploy": "cd docs && npm run build",
    "docs-deploy": "cd docs && ./scripts/deploy.sh",
    "preboilerplate-start": "npm -s run transpile && babel-node scripts/boilerplate.js",
    "boilerplate-start": "cd boilerplate && npm start",
    "preall-tests": "npm run lint",
    "all-tests": "npm run tests && npm run test-boilerplate && npm run docs",
    "test": "npm run all-tests",
    "test-with-coverage": "nyc npm run all-tests",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "release": "npmpub",
    "postrelease": "npm -s run docs-deploy"
  },
  "nyc": {
    "exclude": [
      "**/__tests__/**",
      "**.template.js"
    ],
    "include": [
      "src/**"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-react",
      "babel-preset-es2015",
      "babel-preset-stage-1"
    ],
    "plugins": [
      "babel-plugin-flow-react-proptypes"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": [
      "eslint-config-i-am-meticulous/react-flow"
    ],
    "rules": {
      "react/prefer-stateless-function": 0
    }
  },
  "eslintConfigRuleReact/prefer-stateless-function": "https://github.com/MoOx/phenomic/issues/46",
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "ava": {
    "files": [
      "src/**/__tests__/*.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
